<!-- ============================ Footer =========================== -->

<footer>
    <div class="container">
            <div class="copy">
                <p>
                    &copy; 2019<script>new Date().getFullYear()>2010&&document.write("-"+new Date().getFullYear());</script>, Content By <%- config.author %>. All Rights Reserved.
                </p>

            </div>
            <div class="social">
                <ul>
                    <% if(theme.github){ %>
                    <li><a href="<%- theme.github %>" title="Github" target="_blank"><i class="icon-github"></i></a>&nbsp;</li>
                    <% } %>
                    <% if(theme.instagram){ %>
                    <li><a href="<%- theme.instagram %>" title="Instagram" target="_blank"><i class="icon-instagram"></i></a>&nbsp;</li>
                    <% } %>
                    <% if(theme.linkedin){ %>
                    <li><a href="<%- theme.linkedin %>" title="Linkedin" target="_blank"><i class="icon-linkedin"></i></a>&nbsp;</li>
                    <% } %>
                    <% if(theme.google){ %>
                    <li><a href="<%- theme.google %>" title="Google-Plus" target="_blank"><i class="icon-google-plus"></i></a>&nbsp;</li>
                    <% } %>
                    <% if(theme.weibo){ %>
                    <li><a href="<%- theme.weibo %>" title="Sina-Weibo" target="_blank"><i class="icon-sina-weibo"></i></a>&nbsp;</li>
                    <% } %>
                </ul>
            </div>
            <div class="clearfix"> </div>
        </div>
</footer>

<!-- ============================ END Footer =========================== -->